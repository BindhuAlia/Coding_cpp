#include<iostream>
using namespace std;

int main()
{
    int a,r;
    cout<<"enter number";
    cin>>a;
    int temp=a;
   int ce=0,co=0;
    while(a!=0)
    {
    r=a%10;
    a=a/10;
    if(r%2==0)
    ce++;
    else
    co++;
    
    }
    cout<<"even numbers are "<<ce;
    cout<<"odd numbers are "<<co;
    return 0;
}
