#include<iostream>
using namespace std;

int main()
{
    int n;
    cout<<"enter number=";
    cin>>n;
    int sum =0;
    while(n!=0)
    {
     int r=r%10;
    n=n/10;
     sum=sum+r*r;
    if(n==0){
    n=sum;
    sum=0;
    }
    if(n==1)
    {
    cout<<"Happy number";
    break;
    }
    else
    cout<<"Not Happy number";
    break;
    }
    return 0;
}
