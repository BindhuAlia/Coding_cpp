#include<iostream>
using namespace std;

int main()
{
    int n;
    cout << "Enter range";
    cin>>n;
    int a[n];
    cout<<"Enter elements";

    //To read elements

      for(int i=0;i<n;i++)
       {
         cin>>a[i];
       }

    //Main code for bubble sort

     for(int i=0; i<n ; i++)
       {
          for(int j=0; j<n-i-1 ; j++)
           { 
              if (a[j]>a[j+1])
                swap(a[j],a[j+1]);
           }
    
       }

    //To print elements

    for(int i=0;i<n;i++)
      {
        cout<<a[i]<<" ";
      }
    
    return 0;
}
